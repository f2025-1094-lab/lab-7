#include <iostream>
using namespace std;
void printTableRange(int start, int end) {
    if (start > end) {
        cout << "Invalid range! Start should be less than or equal to end." << endl;
        return;
    }
    for (int i = start; i <= end; i++) {
        for (int j = 1; j <= 10; j++) {
            cout << i << " x " << j << " = " << i * j << endl;
        }
    }
}
int main() {
    int start, end;
    cout << "Enter start number: ";
    cin >> start;
    cout << "Enter end number: ";
    cin >> end;
    printTableRange(start, end);
    return 0;
}
